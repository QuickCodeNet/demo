            "body": {
              "mode": "raw",
              "raw": "{\r\n  \"email\": \"demo@quickcode.net\",\r\n  \"password\": \"String1!\"\r\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "var json = pm.response.json();",
                  "var token = json.accessToken;",
                  "var refreshToken = json.refreshToken;",
                  "const isEnvSelected = pm.environment.name && pm.environment.name !== \"no_env\";",
                  "if (isEnvSelected) {",
                  "    pm.environment.set(\"access_token\", token);",
                  "    pm.environment.set(\"refresh_token\", refreshToken);",
                  "} else {",
                  "    pm.collectionVariables.set(\"access_token\", token);",
                  "    pm.collectionVariables.set(\"refresh_token\", refreshToken);",
                  "}",
                  "console.log(`Selected Environment: ${pm.environment.name || \"none\"}`);"
                ]
              }
            }
          ]
        }
      ],
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "type": "text/javascript",
            "exec": [
              "const fallback_env = \"local\";",
              "const default_base_url = \"http://localhost:6060\";",
              "",
              "const isEnvSelected = pm.environment.name && pm.environment.name !== \"no_env\";",
              "",
              "let env;",
              "let base_url;",
              "",
              "env = pm.variables.get(\"active_env\") || fallback_env;",
              "",
              "if (isEnvSelected) {",
              "    env = pm.environment.get(\"active_env\");",
              "",
              "    if (!env) {",
              "        env = pm.variables.get(\"active_env\") || fallback_env;",
              "        pm.environment.set(\"active_env\", env);",
              "    }",
              "}",
              "",
              "base_url = pm.variables.get(`base_url_${env}`) || default_base_url;",
              "pm.variables.set(\"base_url\", base_url);",
              "",
              "console.log(`Selected Environment: ${pm.environment.name || \"none\"}`);",
              "console.log(`Resolved active_env: ${env}`);",
              "console.log(`Resolved base_url: ${base_url}`);"
            ]
          }
        }
      ]
